{
  "Routes": [
    {
      "DownstreamPathTemplate": "/Office/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "office.commands.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/office/{everything}",
      "UpstreamHttpMethod": [ "POST", "PATCH", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/Office/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "office.queries.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/office/{everything}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/Doctor/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "doctorapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/doctor/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PATCH", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/Auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/auth/{everything}",
      "UpstreamHttpMethod": [ "POST" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigateway:80"
  }
}
