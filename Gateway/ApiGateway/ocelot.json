{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/auth/{everything}",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/doctor/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "doctorapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/doctor/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PATCH", "DELETE" ]
    },

    {
      "DownstreamPathTemplate": "/api/office/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "officeapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/office/{everything}",
      "UpstreamHttpMethod": ["GET", "POST", "PATCH", "DELETE"]
    },
    {
      "DownstreamPathTemplate": "/api/appointment/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "appointmentapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/appointment/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PATCH", "DELETE" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigateway:80"
  }
}